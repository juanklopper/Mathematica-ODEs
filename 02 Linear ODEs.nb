Cell[CellGroupData[{
Cell["Linear ordinary differential equations", "Title",
 CellChangeTimes->{{3.690203202200038*^9, 3.690203212456306*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "They take the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["dy", "dx"], "+", 
     RowBox[{
      RowBox[{
       SubscriptBox["a", "0"], "(", "x", ")"}], "y"}]}], "=", 
    RowBox[{"g", "(", "x", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Item",
 CellChangeTimes->{{3.690203219887233*^9, 3.690203310756652*^9}}],

Cell[TextData[{
 "In standard form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["dy", "dx"], "+", 
     RowBox[{
      RowBox[{"P", "(", "x", ")"}], "y"}]}], "=", 
    RowBox[{"f", "(", "x", ")"}]}], TraditionalForm]]],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"P", "(", "x", ")"}], "=", 
     FractionBox[
      RowBox[{
       SubscriptBox["a", "0"], "(", "x", ")"}], 
      RowBox[{
       SubscriptBox["a", "1"], "(", "x", ")"}]]}], ",", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "1"], "(", "x", ")"}], "\[NotEqual]", "0"}]}], 
   TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.690203219887233*^9, 3.6902034269640627`*^9}, {
  3.6902648502699614`*^9, 3.690264850270425*^9}}],

Cell[TextData[{
 "From the standard form we solve the ODE by multiplying by the integrating \
factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d", "dx"], "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Integral]", 
        RowBox[{
         RowBox[{"P", "(", "x", ")"}], 
         RowBox[{"\[DifferentialD]", "x"}]}]}]], "y"}], "=", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Integral]", 
        RowBox[{
         RowBox[{"P", "(", "x", ")"}], 
         RowBox[{"\[DifferentialD]", "x"}]}]}]], 
      RowBox[{"f", "(", "x", ")"}]}]}], "]"}], TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.690203219887233*^9, 3.6902034269640627`*^9}, {
  3.690264851785337*^9, 3.690264951085947*^9}, {3.690638131976123*^9, 
  3.690638131976714*^9}}],

Cell[TextData[{
 "The final solution will be in the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     SubscriptBox["y", "c"], "+", 
     SubscriptBox["y", "p"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "c"], " ", "is", " ", "the", " ", "solution", " ", 
    "associated", " ", "with"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " the homogeneous DE and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["y", "p"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the particular solution of the DE"
}], "Item",
 CellChangeTimes->{{3.690203219887233*^9, 3.6902034269640627`*^9}, {
  3.690264851785337*^9, 3.690264951085947*^9}, {3.690638132959597*^9, 
  3.690638180505789*^9}, {3.690638236899495*^9, 3.690638303154497*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Example", "Section",
 CellChangeTimes->{{3.690203470650426*^9, 3.6902034731769342`*^9}}],

Cell[TextData[{
 "Solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["dy", "dx"], "-", 
     RowBox[{"3", "y"}]}], "=", "0"}], TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.6902034800343122`*^9, 3.690203544065422*^9}, {
  3.690203625640061*^9, 3.690203637553801*^9}, {3.690203690862885*^9, 
  3.69020369313981*^9}, {3.6902636223988867`*^9, 3.690263630646044*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"y", "'"}], "[", "x", "]"}], "-", 
      RowBox[{"3", "*", 
       RowBox[{"y", "[", "x", "]"}]}]}], "\[Equal]", "0"}], ",", 
    RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}], "//", 
  "TraditionalForm"}]], "Input",
 CellChangeTimes->{{3.690203555825058*^9, 3.6902035808594217`*^9}, {
  3.690203710579918*^9, 3.690203755535264*^9}, {3.690263117540462*^9, 
  3.6902631191857977`*^9}, {3.690263640212737*^9, 3.69026365176357*^9}, {
  3.6906379697199783`*^9, 3.6906379714449997`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "(", "x", ")"}], "\[Rule]", 
     RowBox[{
      SubscriptBox[
       TagBox["c",
        C], "1"], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"3", " ", "x"}]]}]}], "}"}], "}"}], TraditionalForm]], "Output",\

 CellChangeTimes->{3.690203582240559*^9, 3.690203711922278*^9, 
  3.6902631200863953`*^9, 3.690263653520159*^9, 3.690637973846573*^9}]
}, Open  ]]
}, Open  ]],

